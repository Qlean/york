{
  "name": "york",
  "version": "0.0.1",
  "description": "york - UI library",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf build/* && babel --out-dir build src --copy-files",
    "watch": "rm -rf build/* && babel --source-maps --out-dir build src --copy-files --watch",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qlean/york.git"
  },
  "keywords": [
    "react",
    "react-native",
    "webpack",
    "ui",
    "components",
    "Eboracum",
    "Eburacum",
    "Eburaci"
  ],
  "author": "Qlean frontend team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Qlean/york/issues"
  },
  "homepage": "https://github.com/Qlean/york#readme",
  "dependencies": {
    "@storybook/addon-knobs": "^3.2.13",
    "@storybook/react": "^3.2.13",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "color": "^2.0.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "ignore-loader": "^0.1.2",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "styled-components": "^2.2.3",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "html-webpack-plugin": "^2.30.1",
    "webpack-dev-server": "^2.9.3"
  }
}